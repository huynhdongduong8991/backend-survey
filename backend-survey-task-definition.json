{
    "containerDefinitions": [
        {
            "name": "backend-survey-api",
            "image": "800067646362.dkr.ecr.ap-east-1.amazonaws.com/backend-survey-api",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "backend-survey-api-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/backend-survey-task-definition",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": [],
            "secrets": [
                {
                    "name": "JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "JWT_EXPIRATION_TIME",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "SESSION_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "DB_HOST",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "DB_PORT",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "DB_USER",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "DB_NAME",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "GOOGLE_CLIENT_ID",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "GOOGLE_CLIENT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "GOOGLE_CALLBACK_URL",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "FRONTEND_ENDPOINT",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                },
                {
                    "name": "GROQ_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:ap-east-1:800067646362:secret:dev/survey-COeign"
                }
            ]
        }
    ],
    "family": "backend-survey-task-definition",
    "taskRoleArn": "arn:aws:iam::800067646362:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::800067646362:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
